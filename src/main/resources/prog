import ich3.bin2.ein1.entry


public class Button extends View{

    public client Consumer<View> onClick;
    public server dirty boolean entry;

    public Button(){
        entry = false; //server dirty
    }

    public client void onClick( Consumer<View> consumer ){
        onClick = consumer;
    }

    public server void serverCall(){
        Test.TEST = 2;
    }
}

